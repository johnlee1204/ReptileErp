                    "                    table +='</td>';",
                    "                    table +='<td>';",
                    "                    table +=logData[column];",
                    "                    table +='</td>';",
                    "                    table +='</tr>';",
                    "                }",
                    "                table +='</table>';",
                    "            }",
                    "",
                    "            return table;",
                    "        }",
